  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "^6.0.2",
